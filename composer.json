{
    "name": "joomla/session",
    "type": "joomla-package",
    "description": "Joomla Session Package",
    "keywords": ["joomla", "framework", "session"],
    "homepage": "https://github.com/joomla-framework/session",
    "license": "GPL-2.0+",
    "require": {
        "php": ">=5.3.10",
        "joomla/filter": "~1.0",
        "joomla/event": "~1.1"
    },
    "require-dev": {
        "joomla/database": "~1.0",
        "joomla/test": "~1.0",
        "phpunit/phpunit": "4.*",
        "phpunit/dbunit": "~1.2",
        "squizlabs/php_codesniffer": "1.*"
    },
    "suggest": {
        "joomla/database": "Install joomla/database if you want to use Database session storage."
    },
    "autoload": {
    	"psr-4": {
            "Joomla\\Session\\": "src/"
    	}
    },
    "autoload-dev": {
        "psr-4": {
            "Joomla\\Session\\Tests\\": "tests/"
        }
    }
}
